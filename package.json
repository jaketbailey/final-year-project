{
  "name": "final-year-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "go run main.go",
    "test": "jest --env=jsdom",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint": "eslint src/**/*.js",
    "prettify": "prettier src/**/*.js --write",
    "doc": "jsdoc -c jsdoc.conf.json -r ./src/ -d ./docs/jsdoc/ | godoc -http=:6060 | start http://localhost:6060/pkg/cycling-route-planner/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaketbailey/final-year-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaketbailey/final-year-project/issues"
  },
  "homepage": "https://github.com/jaketbailey/final-year-project#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@testing-library/react": "^14.0.0",
    "create-react-app": "^5.0.1",
    "enzyme": "^3.11.0",
    "eslint-plugin-react": "^7.32.2",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^4.0.2",
    "openrouteservice-js": "^0.3.0",
    "jest-environment-jsdom": "^29.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "eslint": "^8.36.0",
    "eslint-config-portsoc": "^1.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "root": true,
    "env": {
      "browser": true
    }
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run lint:fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy",
      "react-leaflet": "<rootDir>/src/client/__tests/__mocks__/reactLeafletMock.js",
      "leaflet-routing-machine": "<rootDir>/src/client/__tests/__mocks__/leafletRoutingMachineMock.js"
    }
  }
}
