    "test": "jest && go test ./src/back-end/__tests",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint": "eslint src/**/*.js",
    "prettify": "prettier src/**/*.js --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaketbailey/final-year-project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaketbailey/final-year-project/issues"
  },
  "homepage": "https://github.com/jaketbailey/final-year-project#readme",
  "dependencies": {
    "create-react-app": "^5.0.1"
  },
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-portsoc": "^1.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6"
  },
  "eslintConfig": {
    "extends": [
